                    xmlns:viewModels="clr-namespace:ServiceProcess.Helpers.ViewModels">

  <DataTemplate DataType="{x:Type viewModels:ServicesControllerViewModel}">
    <views:ServicesControllerView />
  </DataTemplate>

  <DataTemplate DataType="{x:Type viewModels:ServiceViewModel}">
    <views:ServiceView />
  </DataTemplate>

</ResourceDictionary>